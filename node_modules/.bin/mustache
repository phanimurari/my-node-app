#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/workspace/nodejs/coding-practices/coding-practice-9a-solution/node_modules/.pnpm/mustache@2.3.2/node_modules/mustache/bin/node_modules:/home/workspace/nodejs/coding-practices/coding-practice-9a-solution/node_modules/.pnpm/mustache@2.3.2/node_modules/mustache/node_modules:/home/workspace/nodejs/coding-practices/coding-practice-9a-solution/node_modules/.pnpm/mustache@2.3.2/node_modules:/home/workspace/nodejs/coding-practices/coding-practice-9a-solution/node_modules/.pnpm/node_modules:/home/workspace/nodejs/coding-practices/coding-practice-9a-solution/node_modules:/home/workspace/nodejs/coding-practices/node_modules:/home/workspace/nodejs/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/nodejs/coding-practices/coding-practice-9a-solution/node_modules/mustache/bin/node_modules:/home/workspace/nodejs/coding-practices/coding-practice-9a-solution/node_modules/mustache/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/workspace/nodejs/coding-practices/coding-practice-9a-solution/node_modules/.pnpm/mustache@2.3.2/node_modules/mustache/bin/node_modules:/home/workspace/nodejs/coding-practices/coding-practice-9a-solution/node_modules/.pnpm/mustache@2.3.2/node_modules/mustache/node_modules:/home/workspace/nodejs/coding-practices/coding-practice-9a-solution/node_modules/.pnpm/mustache@2.3.2/node_modules:/home/workspace/nodejs/coding-practices/coding-practice-9a-solution/node_modules/.pnpm/node_modules:/home/workspace/nodejs/coding-practices/coding-practice-9a-solution/node_modules:/home/workspace/nodejs/coding-practices/node_modules:/home/workspace/nodejs/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/nodejs/coding-practices/coding-practice-9a-solution/node_modules/mustache/bin/node_modules:/home/workspace/nodejs/coding-practices/coding-practice-9a-solution/node_modules/mustache/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../mustache/bin/mustache" "$@"
else
  exec node  "$basedir/../mustache/bin/mustache" "$@"
fi
