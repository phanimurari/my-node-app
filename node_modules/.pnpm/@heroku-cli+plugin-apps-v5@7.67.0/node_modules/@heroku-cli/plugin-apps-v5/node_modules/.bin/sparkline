#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/workspace/nodejs/coding-practices/coding-practice-9a-solution/node_modules/.pnpm/sparkline@0.2.0/node_modules/sparkline/bin/node_modules:/home/workspace/nodejs/coding-practices/coding-practice-9a-solution/node_modules/.pnpm/sparkline@0.2.0/node_modules/sparkline/node_modules:/home/workspace/nodejs/coding-practices/coding-practice-9a-solution/node_modules/.pnpm/sparkline@0.2.0/node_modules:/home/workspace/nodejs/coding-practices/coding-practice-9a-solution/node_modules/.pnpm/node_modules:/home/workspace/nodejs/coding-practices/coding-practice-9a-solution/node_modules:/home/workspace/nodejs/coding-practices/node_modules:/home/workspace/nodejs/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/nodejs/coding-practices/coding-practice-9a-solution/node_modules/.pnpm/@heroku-cli+plugin-apps-v5@7.67.0/node_modules/@heroku-cli/plugin-apps-v5/node_modules:/home/workspace/nodejs/coding-practices/coding-practice-9a-solution/node_modules/.pnpm/@heroku-cli+plugin-apps-v5@7.67.0/node_modules/@heroku-cli/node_modules:/home/workspace/nodejs/coding-practices/coding-practice-9a-solution/node_modules/.pnpm/@heroku-cli+plugin-apps-v5@7.67.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/workspace/nodejs/coding-practices/coding-practice-9a-solution/node_modules/.pnpm/sparkline@0.2.0/node_modules/sparkline/bin/node_modules:/home/workspace/nodejs/coding-practices/coding-practice-9a-solution/node_modules/.pnpm/sparkline@0.2.0/node_modules/sparkline/node_modules:/home/workspace/nodejs/coding-practices/coding-practice-9a-solution/node_modules/.pnpm/sparkline@0.2.0/node_modules:/home/workspace/nodejs/coding-practices/coding-practice-9a-solution/node_modules/.pnpm/node_modules:/home/workspace/nodejs/coding-practices/coding-practice-9a-solution/node_modules:/home/workspace/nodejs/coding-practices/node_modules:/home/workspace/nodejs/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/nodejs/coding-practices/coding-practice-9a-solution/node_modules/.pnpm/@heroku-cli+plugin-apps-v5@7.67.0/node_modules/@heroku-cli/plugin-apps-v5/node_modules:/home/workspace/nodejs/coding-practices/coding-practice-9a-solution/node_modules/.pnpm/@heroku-cli+plugin-apps-v5@7.67.0/node_modules/@heroku-cli/node_modules:/home/workspace/nodejs/coding-practices/coding-practice-9a-solution/node_modules/.pnpm/@heroku-cli+plugin-apps-v5@7.67.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../sparkline@0.2.0/node_modules/sparkline/bin/sparkline" "$@"
else
  exec node  "$basedir/../../../../../../sparkline@0.2.0/node_modules/sparkline/bin/sparkline" "$@"
fi
