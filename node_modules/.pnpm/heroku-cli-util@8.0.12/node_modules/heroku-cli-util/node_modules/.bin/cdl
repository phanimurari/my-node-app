#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/workspace/nodejs/coding-practices/coding-practice-9a-solution/node_modules/.pnpm/cardinal@2.1.1/node_modules/cardinal/bin/node_modules:/home/workspace/nodejs/coding-practices/coding-practice-9a-solution/node_modules/.pnpm/cardinal@2.1.1/node_modules/cardinal/node_modules:/home/workspace/nodejs/coding-practices/coding-practice-9a-solution/node_modules/.pnpm/cardinal@2.1.1/node_modules:/home/workspace/nodejs/coding-practices/coding-practice-9a-solution/node_modules/.pnpm/node_modules:/home/workspace/nodejs/coding-practices/coding-practice-9a-solution/node_modules:/home/workspace/nodejs/coding-practices/node_modules:/home/workspace/nodejs/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/nodejs/coding-practices/coding-practice-9a-solution/node_modules/.pnpm/heroku-cli-util@8.0.12/node_modules/heroku-cli-util/node_modules:/home/workspace/nodejs/coding-practices/coding-practice-9a-solution/node_modules/.pnpm/heroku-cli-util@8.0.12/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/workspace/nodejs/coding-practices/coding-practice-9a-solution/node_modules/.pnpm/cardinal@2.1.1/node_modules/cardinal/bin/node_modules:/home/workspace/nodejs/coding-practices/coding-practice-9a-solution/node_modules/.pnpm/cardinal@2.1.1/node_modules/cardinal/node_modules:/home/workspace/nodejs/coding-practices/coding-practice-9a-solution/node_modules/.pnpm/cardinal@2.1.1/node_modules:/home/workspace/nodejs/coding-practices/coding-practice-9a-solution/node_modules/.pnpm/node_modules:/home/workspace/nodejs/coding-practices/coding-practice-9a-solution/node_modules:/home/workspace/nodejs/coding-practices/node_modules:/home/workspace/nodejs/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/nodejs/coding-practices/coding-practice-9a-solution/node_modules/.pnpm/heroku-cli-util@8.0.12/node_modules/heroku-cli-util/node_modules:/home/workspace/nodejs/coding-practices/coding-practice-9a-solution/node_modules/.pnpm/heroku-cli-util@8.0.12/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../cardinal@2.1.1/node_modules/cardinal/bin/cdl.js" "$@"
else
  exec node  "$basedir/../../../../../cardinal@2.1.1/node_modules/cardinal/bin/cdl.js" "$@"
fi
